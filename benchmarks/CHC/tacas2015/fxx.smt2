(set-logic HORN)
(declare-fun P4 (Int Int) Bool)
(declare-fun P0 (Int) Bool)
(declare-fun P1 (Int Int) Bool)
(declare-fun P2 (Int) Bool)
(declare-fun P3 (Int Int Int) Bool)
(assert (forall ((x3 Int) (x4 Int) (x0 Int) (x1 Int) (x2 Int)) (=> (and (P0 x3) (P1 x3 x4) (or (not (> x1 0)) (not (> x2 0))) (or (and (> x1 0) (> x3 0)) (and (not (> x1 0)) (not (> x3 0)))) (or (and (> x2 0) (<= x4 0)) (and (not (> x2 0)) (not (<= x4 0))))) (P3 x3 x4 x0))))
(assert (forall ((x0 Int)) (=> (P2 x0) (P0 x0))))
(assert (forall ((x0 Int) (x1 Int)) (=> (and (P2 x0) (P2 x1) (= x0 x1)) (P1 x0 x1))))
(assert (forall ((x0 Int) (x1 Int)) (=> (and (P2 x0) (P3 x0 x0 x1)) (P4 x0 x1))))
(assert (forall ((x0 Int) (x1 Int)) (=> (and (P0 x0) (P1 x0 x1) (>= x0 1) (<= x1 0)) false)))
(check-sat)
